<p id="notice"><%= notice %></p>

<h1>Training Executions</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Topic</th>
      <th>Objective</th>
      <th>Date</th>
      <th>Hourly intensity</th>
      <th>Trainer</th>
      <th>Internal</th>
      <th>Area</th>
      <th>Collaborator</th>
      <th>Company</th>
      <th>Planned training</th>
      <th>Planned</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @training_executions.each do |training_execution| %>
      <tr>
        <td><%= training_execution.category %></td>
        <td><%= training_execution.topic %></td>
        <td><%= training_execution.objective %></td>
        <td><%= training_execution.date %></td>
        <td><%= training_execution.hourly_intensity %></td>
        <td><%= training_execution.trainer %></td>
        <td><%= training_execution.internal %></td>
        <td><%= training_execution.area_id %></td>
        <td><%= training_execution.collaborator_id %></td>
        <td><%= training_execution.company_id %></td>
        <td><%= training_execution.planned_training_id %></td>
        <td><%= training_execution.planned %></td>
        <td><%= link_to 'Show', training_execution %></td>
        <td><%= link_to 'Edit', edit_training_execution_path(training_execution) %></td>
        <td><%= link_to 'Destroy', training_execution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Training Execution', new_training_execution_path %>
